{
  "name": "meristem-core",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "start": "bun run src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target bun",
    "typecheck:run:stable": "tsc --noEmit -p tsconfig.json",
    "typecheck:run:next": "bun run src/cli/typecheck-next.ts",
    "runtime:run:check": "bun run src/cli/runtime-check.ts",
    "benchmark:run:baseline": "MERISTEM_WORKSPACE_ROOT=.. meristem-tooling bench baseline",
    "benchmark:run:pack": "MERISTEM_WORKSPACE_ROOT=.. meristem-tooling bench pack",
    "benchmark:run:pack:full": "MERISTEM_WORKSPACE_ROOT=.. meristem-tooling bench pack --with-http --with-reliability",
    "benchmark:run:http": "MERISTEM_WORKSPACE_ROOT=.. meristem-tooling bench http-matrix --requests 400 --concurrency 40 --warmup 40 --timeout-ms 3000",
    "test:run:integration:core": "MERISTEM_WORKSPACE_ROOT=.. NATS_URL=nats://localhost:4222 meristem-tooling test integration-core",
    "jwt:run:rotate": "bun run src/cli/jwt-rotate.ts rotate",
    "jwt:run:prune": "bun run src/cli/jwt-rotate.ts prune"
  },
  "dependencies": {
    "@insnh-gd/meristem-shared": "file:../meristem-shared",
    "@elysiajs/jwt": "^1.4.0",
    "@iarna/toml": "^2.2.5",
    "bcrypt": "^6.0.0",
    "elysia": "^1.0.0",
    "effect": "^3.12.0",
    "jose": "^6.1.3",
    "mongodb": "^6.0.0",
    "nats": "^2.29.3"
  },
  "devDependencies": {
    "@insnh-gd/meristem-tooling": "npm:@jsr/insnh-gd__meristem-tooling@^0.1.0",
    "@types/bun": "latest",
    "pino": "^10.3.0",
    "typescript": "^5.0.0"
  }
}
